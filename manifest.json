{
  "manifest_version": 3,
  "name": "MikkeBot",
  "version": "1.0",
  "description": "TW auto jobs bot",
  "permissions": ["activeTab"],
  "background": {
    "service_worker": "Mikke.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
